#%Module1.0#####################################################################
##
## modules libs/sundials-2.6.2
##
## modulefiles/libs/sundials-2.6.2
##
proc ModulesHelp { } {
        global version modroot

        puts stderr "libs/sundials-2.6.2"
}

module-whatis   "libs/sundials-2.6.2"

# for Tcl script use only
set     topdir          /opt/packages/sundials-2.6.2/bin
set     version         2.6.2
set     sys             linux86
setenv	SUNDIALS_DIR	$topdir/lib
setenv	SUNDIALS_INC	$topdir/include

prepend-path    PATH            $topdir/include
prepend-path    LD_LIBRARY_PATH $topdir/lib
